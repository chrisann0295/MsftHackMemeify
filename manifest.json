{
"manifest_version": 2,
    "name": "Memeify",
    "description": "Converting all your boring faces to memes",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}